<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentWarlordMonetizedStats">

    <TextView
        android:id="@+id/label_num_banner_swiped"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:textAlignment="center"
        android:layout_marginLeft="10dp"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="@string/label_for_banner_count"
         />

    <TextView
        android:id="@+id/val_num_banner_swiped"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginLeft="250dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="NO" />

    <TextView
        android:id="@+id/label_num_videos_watched"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="10dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="@string/label_for_video_count" />

    <TextView
        android:id="@+id/val_num_videos_watched"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="25dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="no" />

    <TextView
        android:id="@+id/label_num_total_currency"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="10dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="@string/label_for_total_currency" />

    <TextView
        android:id="@+id/val_num_total_currency"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="50dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="N0000" />


    <TextView
        android:id="@+id/label_num_total_damage"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginTop="75dp"
        android:layout_marginLeft="10dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="@string/label_for_total_damage" />

    <TextView
        android:id="@+id/val_num_total_damage"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="75dp"
        android:textAlignment="center"
        android:background="@color/colorPrimaryDark"
        android:textColor="@android:color/background_light"
        android:text="N0000" />

</FrameLayout>